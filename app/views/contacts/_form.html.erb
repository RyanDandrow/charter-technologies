<%= form_for @contact do |f| %>
	<div class="container-fluid">
	    <div class="row">
	        <div class="col-lg-5 col-md-4 col-sm-4">
	            <div class="form-group">          
	                <%= f.label "Company:", class: "form-label" %>
	                <%= f.text_field :company, class: "form-control", id: "company" %>
	                <%= f.select :company_type, ['', 'LLC', 'DBA', 'INC'], class: "form-control suffix" %>
	            </div>
	        </div>

            <div class="col-lg-2 col-md-2 col-sm-3">
                <div class="form-group">
                    <%= f.label :tax_id, 'Tax ID #', class: "form-label" %>
                    <%= f.text_field :tax_id, class: "form-control", id: "tax_id" %>
                    <script type="text/javascript">
                        $(document).ready(function(){
                            $('#tax_id').mask('00-0000000', {placeholder: "00-0000000"});
                        });
                    </script>
                </div>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-3">
                <div class="form-group">
                    <%= f.label :drivers_License, 'Drivers License #', class: "form-label" %>
                    <%= f.text_field :drivers_license, class: "form-control" %>
                 </div>
            </div>
	        <div class="col-lg-2 col-md-2 col-sm-2">
	            <div class="form-group">          
	                <%= f.label :sf_id, "SF ID#", class: "form-label" %>
	                <%= f.text_field :sf_id, class: "form-control" %>
	            </div>
	        </div>
	    </div>

	    <div class="row">
	        <div class="col-lg-5 col-md-4 col-sm-4">
	            <%= f.label :name, 'Name:', class: "form-label" %>
	            <div class="form-group" id="highlight2">  
	                <%= f.text_field :first_name, class: "form-control first" %>
	                <%= f.text_field :middle_name, class: "form-control middle" %>
	                <%= f.text_field :last_name, class: "form-control last" %>
	                <%= f.select :name_suffix, ['', 'Sr', 'Jr', 'II', 'III'], class: "suffix" %>
	            </div>
	        </div>
	        <div class="col-lg-2 col-md-2 col-sm-3">
	            <div class="form-group">          
	                <%= f.label "Goes by:", class: "form-label" %>
	                <%= f.text_field :goes_by, class: "form-control" %>
	            </div>
	        </div>

	        <div class="col-md-2 col-sm-3">
	            <div class="form-group">
	                <%= f.label :ssn, 'SSN#' %>
	                <br>
	                <button type="button" class="btn btn-default btn-sm" data-toggle="modal" data-target="#myModal">
	                     Social Security Number
	                </button>
	            </div>
	        </div>

	        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
	            <div class="modal-dialog" role="document">
	                <div class="modal-content">
	                    <div class="modal-header">
	                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
	                        <h4 class="modal-title" id="myModalLabel">Social Security Social</h4>
	                    </div>
	                    <div class="modal-body">
	                        <div class="row">
	                             <div class="col-md-2 col-sm-2 col-md-offset-3">
	                                <div class="form-inline">
	                                    <%= f.text_field :ssn, class: "form-control", id: "social" %>
	                                    <script type="text/javascript">
	                                        $(document).ready(function(){
	                                            $('#social').mask('000-00-0000', {placeholder: "000-00-0000"});
	                                        });
	                                    </script>
	                                </div>
	                            </div>
	                        </div>
	                    </div>
	                    <div class="modal-footer">
	                        <button type="button" class="btn btn-primary">Save changes</button>
	                    </div>
	                </div>
	            </div>
	        </div>

	         <div class="col-lg-2 col-md-2 col-sm-2 col-sx-2">
	            <div class="form-group">
	                <%= f.label :date_of_birth, 'D.O.B.', class: "form-label" %>
	                <%= f.text_field :date_of_birth, class: "form-control", id: "date" %>
	                <script type="text/javascript">
	                    $(document).ready(function(){
	                        $('#date').mask('00/00/0000', {placeholder: "mm/dd/yyyy"});
	                    });
	                </script>
	            </div>
	        </div>
	    </div>

	    <div class="row">
	        <div class="col-lg-5 col-md-4 col-sm-4">
	        <%= f.label 'Spouse Name:', class: "form-label" %>
	            <div class="form-group">
	                <%= f.text_field :spouse_first_name, class: "form-control first" %>
	                <%= f.text_field :spouse_middle_name, class: "form-control middle" %>
	                <%= f.text_field :spouse_last_name, class: "form-control last" %>
	            </div>
	        </div>
            <div class="col-lg-2 col-md-2 col-sm-3">
                <div class="form-group">
                    <%= f.label :spouse_goes_by, 'Spouse Goes By:', class: "form-label" %>
                    <%= f.text_field :spouse_goes_by, class: "form-control" %>
                </div>
            </div>

	        <div class="col-md-2 col-sm-3">
	            <div class="form-group">
	                <%= f.label :spouse_ssn, 'Spouse SSN#' %>
	                <br>
	                <button type="button" class="btn btn-default btn-sm" data-toggle="modal" data-target="#myModal">
	                     Social Security Number
	                </button>
	            </div>
	        </div>

	        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
	            <div class="modal-dialog" role="document">
	                <div class="modal-content">
	                    <div class="modal-header">
	                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
	                        <h4 class="modal-title" id="myModalLabel">Social Security Social</h4>
	                    </div>
	                    <div class="modal-body">
	                        <div class="row">
	                             <div class="col-md-3 col-sm-3 col-md-offset-3">
	                                <div class="form-inline">
	                                    <%= f.text_field :spouse_ssn, class: "form-control", id: "social" %>
	                                    <script type="text/javascript">
	                                        $(document).ready(function(){
	                                            $('#social').mask('000-00-0000', {placeholder: "000-00-0000"});
	                                        });
	                                    </script>
	                                </div>
	                            </div>
	                        </div>
	                    </div>
	                    <div class="modal-footer">
	                        <button type="button" class="btn btn-primary">Save changes</button>
	                    </div>
	                </div>
	            </div>
	        </div>

			<div class="col-lg-2 col-md-2 col-sm-2">
	            <div class="form-group">
	                <%= f.label :spouse_date_of_birth, 'D.O.B.', class: "form-label" %>
	                <%= f.text_field :spouse_date_of_birth, class: "form-control", id: "date2" %>
	                <script type="text/javascript">
	                    $(document).ready(function(){
	                        $('#date2').mask('00/00/0000', {placeholder: "mm/dd/yyyy"});
	                    });
	                </script>
	            </div>
	        </div>
		</div>
	</div>

    <div class="row">
        <div class="col-lg-2 col-md-2">
            <div class="form-group">          
                <%= f.label "Cell:", class: "form-label" %>
                <%= f.text_field :cell_phone, class: "form-control", id: "phone1" %>
                <script type="text/javascript">
                    $(document).ready(function(){
                        $('#phone1').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
                    });
                </script>
            </div>
            <div class="form-group">          
                <%= f.label "Spouse Cell:", class: "form-label" %>
                <%= f.text_field :spouse_cell_phone, class: "form-control", id: "phone2" %>
           		<script type="text/javascript">
                    $(document).ready(function(){
                        $('#phone2').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
                    });
                </script>
            </div>
	        <div class="form-group">
	            <%= f.label :company_phone, 'Company Phone:', class: "form-label" %>
	            <%= f.text_field :company_phone, class: "form-control", id: "phone3" %>
	            <script type="text/javascript">
	                $(document).ready(function(){
	                    $('#phone3').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
	                });
	            </script>
	        </div>
	        <div class="form-group">
	            <%= f.label :home_phone, 'Home Phone:', class: "form-label" %>
	            <%= f.text_field :home_phone, class: "form-control", id: "phone4" %>
	            <script type="text/javascript">
	                $(document).ready(function(){
	                    $('#phone4').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
	                });
	            </script>
	        </div>
			<div class="form-group">
                <%= f.text_field :alt_phone_1_place, placeholder: 'Alt Phone 1:', class: "form-label" %>
                <%= f.text_field :alt_phone_1, class: "form-control", id: "altphone" %>
                <script type="text/javascript">
                    $(document).ready(function(){
                        $('#altphone').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
                    });
                </script>
	        </div>
	        <div class="form-group">
                <%= f.text_field :alt_phone_2_place, placeholder: 'Alt Phone 2:', class: "form-label" %>
                <%= f.text_field :alt_phone_2, class: "form-control", id: "altphone2" %>
                <script type="text/javascript">
                    $(document).ready(function(){
                        $('#altphone2').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
                    });
                </script>
	        </div>
	        <div class="form-group">
                <%= f.text_field :alt_phone_3_place, placeholder: 'Alt Phone 3:', class: "form-label" %>
                <%= f.text_field :alt_phone_3, class: "form-control", id: "altphone3" %>
                <script type="text/javascript">
                    $(document).ready(function(){
                        $('#altphone3').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
                    });
                </script>
	        </div>
	        <div class="form-group">
                <%= f.text_field :alt_phone_4, placeholder: 'Alt Phone 4:', class: "form-label" %>
                <%= f.text_field :alt_phone_4_place, class: "form-control", id: "altphone4" %>
                <script type="text/javascript">
                    $(document).ready(function(){
                        $('#altphone4').mask('(000)-000-0000', {placeholder: "(000)-000-0000"});
                    });
                </script>
	        </div>
	    </div>

			<div class="col-md-3 col-sm-4 col-md-offset-1">
		        <%= f.label :companyNameBox1, "Address Box 1:" %>
		            <div class="form-group" id="addressBorder">
		            	  <%= f.text_field :companyNameBox1, placeholder: 'Company Name:', class: "form-control" %>
		                  <%= f.text_field :address_line1_box1, placeholder: 'Address Line 1:', class: "form-control" %>
		                  <%= f.text_field :address_line2_box1, placeholder: 'Address Line 2:', class: "form-control" %>
		                  <%= f.text_field :address_line3_box1, placeholder: 'Address Line 3:', class: "form-control" %>
		                  <%= f.text_field :county_box1, placeholder: 'County:', class: "form-control" %>
		                  <%= f.text_field :notes_box1, placeholder: 'Notes:', class: "form-control" %>
		            </div>
		        </div>
		        <div class="col-md-3 col-sm-4 col-md-offset-1">
		        <%= f.label :companyNameBox1, "Address Box 2:" %>
		            <div class="form-group" id="addressBorder">
		            	  <%= f.text_field :contactNamesBox2, placeholder: 'Names on Address:', class: "form-control" %>
		                  <%= f.text_field :address_line1_box2, placeholder: 'Address Line 1:', class: "form-control" %>
		                  <%= f.text_field :address_line2_box2, placeholder: 'Address Line 2:', class: "form-control" %>
		                  <%= f.text_field :address_line3_box2, placeholder: 'Address Line 3:', class: "form-control" %>
		                  <%= f.text_field :county_box2, placeholder: 'County:', class: "form-control" %>
		                  <%= f.text_field :notes_box2, placeholder: 'Notes:', class: "form-control" %>
		            </div>
		        </div>
		        <div class="col-md-3 col-sm-4 col-md-offset-1">
		        <%= f.label :companyNameBox1, "Address Box 3:" %>
		            <div class="form-group" id="addressBorder">
		              <%= f.text_field :additionalAddressBox3, placeholder: 'Additional Address:', class: "form-control" %>
		              <%= f.text_field :address_line1_box3, placeholder: 'Address Line 1:', class: "form-control" %>
		              <%= f.text_field :address_line2_box3, placeholder: 'Address Line 2:', class: "form-control" %>
		              <%= f.text_field :address_line3_box3, placeholder: 'Address Line 3:', class: "form-control" %>
		              <%= f.text_field :county_box3, placeholder: 'County:', class: "form-control" %>
		              <%= f.text_field :notes_box3, placeholder: 'Notes:', class: "form-control" %>
		            </div>
		        </div>
		        <div class="col-md-3 col-sm-4 col-md-offset-1">
		        <%= f.label :companyNameBox1, "Address Box 4:" %>
		            <div class="form-group" id="addressBorder">
		              <%= f.text_field :additionalAddressBox3, placeholder: 'Additional Address:', class: "form-control" %>
		              <%= f.text_field :address_line1_box4, placeholder: 'Address Line 1:', class: "form-control" %>
		              <%= f.text_field :address_line2_box4, placeholder: 'Address Line 2:', class: "form-control" %>
		              <%= f.text_field :address_line3_box4, placeholder: 'Address Line 3:', class: "form-control" %>
		              <%= f.text_field :county_box4, placeholder: 'County:', class: "form-control" %>
		              <%= f.text_field :notes_box4, placeholder: 'Notes:', class: "form-control" %>
		            </div>
		        </div>
		        <div class="col-md-3 col-md-offset-3">     

		                <%= f.label "Select State:" %>
		                <%= select_tag :us_states, options_for_select(us_states), class: "combobox form-control" %>
		                <script type="text/javascript">
		                  $(document).ready(function(){
		                    $('.combobox').combobox();
		                  });
		                </script>

		        </div>
	   
	</div>
	
    <div class="row">
        <div class="col-md-5 col-sm-4">
            <div class="form-group">
                <%= f.label :email, 'Email:', class: "form-label" %>
                <%= f.text_field :email, class: "form-control" %>
            </div>
        </div>         
        <div class="col-md-5 col-sm-4 col-lg-offset-1">
            <div class="form-group">
                <%= f.label :spouse_email, 'Spouse Email:', class: "form-label" %>
                <%= f.text_field :spouse_email, class: "form-control" %>
            </div>
        </div> 
    </div>

    <div class="row">
        <div class="col-md-5 col-sm-4">
            <div class="form-group">
                <%= f.label :web_address, 'Web Address:', class: "form-label" %>
                <%= f.text_field :web_address, class: "form-control" %>
            </div>
        </div>   
        <div class="col-md-5 col-sm-4 col-lg-offset-1">
            <div class="form-group">
                <%= f.text_field :alt_email_label, placeholder: 'Alt Email:', class: "form-label-3" %>
                <%= f.text_field :alt_email, class: "form-control" %>
            </div>
        </div>    
    </div>
		
	<div class="row">
		<div class="col-lg-1">
			<div class="form-group">
					<%= f.label "Urgent:" %>
				<%= f.select :urgent, ['No', 'Yes'] %>
			</div>
		</div>
		<div class="col-md-10">
			<div class="form-group">
				<%= f.label "Additional info:" %>
				<%= f.text_area :additional_info, class: "form-control", :rows => 5 %>
			</div>
		</div>
	</div>


	<p class="submits">
		<%= link_to 'Cancel', contacts_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Add new contact", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>
